{
  "extends": ["airbnb-base", "plugin:jest/recommended"],
  "env": {
    "jest/globals": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "test": true,
    "fetch": true,
  },
  "plugins": [
    "header",
    "jest"
  ],
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "no-console": ["error", { "allow": ["warn", "error"] }],
    "quote-props": ["error", "consistent"],
  }
}
